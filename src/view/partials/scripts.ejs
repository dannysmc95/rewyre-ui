<!-- Vue 3 -->
<script src="https://unpkg.com/vue@next"></script>

<!-- Custom Scripts -->
<script type="text/javascript" src="/ui/assets/scripts/sidebar.js"></script>

<!-- Custom -->
<% scripts.forEach(script => { %>
	<% if (typeof script.raw === 'undefined') { %>
		<script type="<%= script.type %>" src="<%= script.src %>"></script>
	<% } else { %>
		<script type="<%= script.type %>">
			<%- script.raw %>
		</script>
	<% } %>
<% }); %>
